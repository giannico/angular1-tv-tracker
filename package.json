{
  "name": "angular1-tv-tracker",
  "version": "0.1.0",
  "description": "A sample project for an angular 1.x application with TypeScript, webpack and Firebase support.",
  "main": "/",
  "scripts": {
    "start": "npm run server",
    "clean-dist": "rimraf dist",
    "prebuild": "npm run clean-dist",
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "lint": "tslint -c tslint.json \"./client/**/*.ts\"",
    "postinstall": "typings install",
    "firebase-deploy": "npm run build && firebase deploy"
  },
  "author": "Andre Giannico <andre@andregiannico.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-loading-bar": "^0.9.0",
    "angular-toastr": "^2.1.1",
    "angular-ui-bootstrap": "^2.1.3",
    "angular-ui-router": "^1.0.0-beta.1",
    "angularfire": "^2.0.2",
    "bootstrap": "^3.3.7",
    "concurrently": "^2.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "firebase": "3.3.0",
    "html-webpack-plugin": "^2.21.0",
    "jsonfile": "^2.3.1",
    "lodash": "^4.15.0",
    "ng-annotate-loader": "^0.1.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "sync-request": "^3.0.1",
    "ts-loader": "^0.8.2",
    "typings": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
